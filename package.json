{
  "name": "softopshub",
  "homepage": "https://softopshub.github.io/softopshub.com/",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && cp dist/index.html dist/404.html",
    "build:admin": "cd ../p2pfood-admin && npm run build",
    "build:all": "npm run build && npm run build:admin && mkdir -p dist/p2pfood-admin && cp -r ../p2pfood-admin/out/* dist/p2pfood-admin/",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview:all": "npm run build:all && cd dist && python3 -m http.server 8000",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@material-tailwind/react": "^2.1.10",
    "@tailwindcss/vite": "^4.0.6",
    "clsx": "^2.1.1",
    "flowbite": "^3.1.1",
    "framer-motion": "^12.0.6",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.475.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.4.0",
    "react-leaflet": "^5.0.0",
    "react-router-dom": "^7.1.5",
    "react-slick": "^0.30.3",
    "slick-carousel": "^1.8.1",
    "tailwindcss": "^4.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "gh-pages": "^6.3.0",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "vite": "^6.0.5",
    "vite-plugin-image-optimizer": "^1.1.8",
    "vite-plugin-static-copy": "^2.2.0"
  }
}
